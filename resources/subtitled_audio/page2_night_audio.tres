metadata/_custom_type_script = "uid://7s0p2xf8cv0r"

[sub_resource type="Resource" id="Resource_trdvb"]
script = ExtResource("3_53qgr")
text = "Завтра я напишу о нашей прогулке, спешу, скоро надо идти на вокзал встречать..."
